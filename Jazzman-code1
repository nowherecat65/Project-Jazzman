  const audio = document.getElementById("audio");
  if (audio.paused) {
    audio.play();
    playPauseButton.src = "https://i.imgur.com/OAe5dOF.png";  // Set the pause image
  } else {
    audio.pause();
    playPauseButton.src = "https://i.imgur.com/OAe5dOF.png";  // Set the play image
  }
});

