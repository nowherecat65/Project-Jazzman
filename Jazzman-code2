cyoAdventure = function(a) {
    update_EV_dict();
    ans = campaignTrail_temp.player_answers[campaignTrail_temp.player_answers.length - 1]
    if (ans == 210001 && ev_dict["77"] < 269 && DoleNom == 1) {
        Dolewin=true;
        update_EV_dict();
        campaignTrail_temp.question_number = 49;
        
        console.log(Dolewin = true);
    } else if (ans == 210002 && ev_dict["77"] < 269 && DoleNom == 1) {
        Dolewin=true;
        update_EV_dict();
        campaignTrail_temp.question_number = 49;
