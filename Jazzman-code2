      } else if (LibSocial == 2 && GOPSupermajority == 1 && ans == 160003) {
          page_list[0][13] = `<span style="font-weight: bold; color: red; font-size: 24px;"> Pat Buchanan</span>`; // Style ExpectedOpponent
        } if (LibSocial == 3 && GOPSupermajority == 1 && ans == 160002) {
          page_list[0][13] = `<span style="font-weight: bold; color: red; font-size: 24px;"> Lamar Alexander</span>`; // Style ExpectedOpponent
      } else if (LibSocial == 3 && GOPSupermajority == 1 && ans == 160003) {
