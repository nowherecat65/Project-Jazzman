      } else if (SlimGOPCongress == 1 && ans == 160002){ 
          page_list[0][13] = `<span style="font-weight: bold; color: red; font-size: 24px;"> Bob Dole</span>`; // Style ExpectedOpponent
      } else if (SlimGOPCongress == 1 && ans == 160003){ 
          page_list[0][13] = `<span style="font-weight: bold; color: red; font-size: 24px;"> Bob Dole</span>`; // Style ExpectedOpponent
      }    else if (LibSocial <= 1 && GOPSupermajority == 1 && ans == 160002){ 
          page_list[0][13] = `<span style="font-weight: bold; color: red; font-size: 24px;"> Bob Dole</span>`; // Style ExpectedOpponent
      } else if (LibSocial <= 1 && GOPSupermajority == 1 && ans == 160003){ 
          page_list[0][13] = `<span style="font-weight: bold; color: red; font-size: 24px;"> Bob Dole</span>`; // Style ExpectedOpponent
      } else if (LibSocial == 2 && RepublicanRevolution == 1 && ans == 160002) {
          page_list[0][13] = `<span style="font-weight: bold; color: red; font-size: 24px;"> Arlen Specter</span>`; // Style ExpectedOpponent
    } else if (LibSocial == 2 && RepublicanRevolution == 1 && ans == 160003) {
          page_list[0][13] = `<span style="font-weight: bold; color: red; font-size: 24px;"> Arlen Specter</span>`; // Style ExpectedOpponent
        } if (WarScore <= 0 && page_list[0][6] == "War Situation:") {
        page_list[0][7] = `<span style="font-weight: bold; color: red; font-size: 24px;"> Dire</span>`; // Style WarSituation
         } else if (WarScore >= 1 && WarScore <= 3 && page_list[0][6] == "War Situation:") {
        page_list[0][7] = `<span style="font-weight: bold; color: red; font-size: 24px;"> Poor</span>`; // Style WarSituation
         } if (NatSecurity <= 0 && page_list[0][10] == "Homeland Security:") {
          page_list[0][11] = `<span style="font-weight: bold; color: red; font-size: 24px;"> Severely Threatened</span>`; // Style HomelandSecurity
         } else if (NatSecurity >= 1 && NatSecurity <= 3 && page_list[0][10] == "Homeland Security:") {
          page_list[0][11] = `<span style="font-weight: bold; color: red; font-size: 24px;"> Threatened</span>`; // Style HomelandSecurity
         } 
        
        
        
        
  }
      // Update page_list before rendering
      updatePageList();

       // Create the header
    let headerDiv = createHeader(parentDiv, toggleDiv, page_list);
    parentDiv.appendChild(headerDiv);
parentDiv.style.backgroundImage = 'url("https://i.imgur.com/pySkSD7.png")'; // Add background image
    // Create the container for the page content
   // For the body content, create a retro-looking content box
const textContainer = document.createElement('div');
textContainer.style.width = '80%';
textContainer.style.height = '300px'; // Max height to trigger scroll
textContainer.style.margin = '20px auto';
textContainer.style.border = '3px solid #000'; // Thick black border like a computer screen frame
textContainer.style.borderRadius = '0'; // No rounded corners, flat screen style
textContainer.style.backgroundColor = '#D3D3D3'; // Light gray for the screen background (fallback color)
textContainer.style.backgroundSize = 'cover'; // Ensure the image covers the entire container
textContainer.style.backgroundPosition = 'center'; // Center the image
textContainer.style.padding = '20px';
textContainer.style.boxShadow = '0 4px 8px rgba(0, 0, 0, 0.2)';
textContainer.style.overflowY = 'auto'; // Auto scroll when content overflows
textContainer.style.maxHeight = '300px'; // Ensure content overflows only when necessary
textContainer.style.display = 'block'; // Block layout for better scrolling behavior
textContainer.style.fontFamily = 'Courier New, monospace'; // Monospaced font for the terminal look
textContainer.style.fontSize = '14px'; // Classic size for text on old computer screens
textContainer.style.color = '#000'; // Black text for readability

// Apply the scrollbar styles (for modern browsers)
textContainer.style.scrollbarWidth = 'thin'; // For Firefox
textContainer.style.scrollbarColor = '#0000FF #D3D3D3'; // Blue scrollbar with gray background for Firefox

// For Webkit-based browsers (Chrome, Safari, etc.)
const style = document.createElement('style');
style.innerHTML = `
    /* Custom scrollbar styles for Webkit browsers */
    div::-webkit-scrollbar {
        width: 12px; /* Set width of the scrollbar */
    }

    div::-webkit-scrollbar-track {
        background: #D3D3D3; /* Light gray track */
    }

    div::-webkit-scrollbar-thumb {
        background: #0000FF; /* Blue scrollbar thumb */
        border-radius: 6px;
    }

    div::-webkit-scrollbar-thumb:hover {
        background: #1E90FF; /* Darker blue when hovered */
    }
`;
document.head.appendChild(style);
parentDiv.appendChild(textContainer);

    // Function to render content for the current page
    function renderPage(pageIndex) {
        textContainer.innerHTML = ''; // Clear previous content
        const pageContent = page_list[pageIndex];
        
        pageContent.forEach(content => {
            const paragraph = document.createElement('p');
            paragraph.innerHTML = content;
            paragraph.style.fontSize = '20px';
            paragraph.style.margin = '10px 0';
            paragraph.style.textAlign = 'center'; // Center align text
            paragraph.style.fontWeight = 'bold'; // Bold text for emphasis
            textContainer.appendChild(paragraph);
        });
    }

    // Initial page render
    renderPage(current_page);

    // Function to create the header
  function createHeader(parentDiv, toggleDiv, page_list) {
    let headerDiv = document.createElement('div');
    headerDiv.style.display = 'flex';
    headerDiv.style.color = 'black'; // Black text for the title
    headerDiv.style.backgroundColor = '#A9A9A9'; // Gray background for the "computer" look
    headerDiv.style.border = '3px solid #000'; // Thick black border for the computer screen effect
    headerDiv.style.borderRadius = '0'; // Sharp corners for that retro feel
    headerDiv.style.padding = '8px 12px'; // Tight padding
    headerDiv.style.width = '80%'; // Set the width to 80% of the page
    headerDiv.style.maxWidth = '100%'; // Max width at 100% of its container
    headerDiv.style.margin = '0 auto'; // Center the header div horizontally
    headerDiv.style.alignItems = 'center'; // Center items vertically

    // Create a div to wrap the title and center it
    let titleWrapper = document.createElement('div');
    titleWrapper.style.flex = '1'; // Let it take up remaining space
    titleWrapper.style.textAlign = 'center'; // Center the text inside it
    titleWrapper.style.display = 'flex';
    titleWrapper.style.justifyContent = 'center'; // Ensure the title is horizontally centered
    titleWrapper.appendChild(createHeaderText('Government')); // Add the title

    // Create the left section for the back button
    let leftSection = document.createElement('div');
    leftSection.style.flex = '0 0 auto'; // Prevent it from stretching
    leftSection.appendChild(createBackButton(parentDiv, toggleDiv));

    // Append both sections to the header
    headerDiv.appendChild(leftSection);
    headerDiv.appendChild(titleWrapper); // Append the title wrapper

    return headerDiv;
}
    // Create a back button to exit
   function createBackButton(parentDiv, toggleDiv) {
    let backButton = document.createElement('button');
    backButton.innerText = 'Back';
    backButton.style.margin = '5px';
    backButton.style.backgroundColor = '#D3D3D3'; // Light gray background for that classic computer feel
    backButton.style.color = '#000'; // Black text
    backButton.style.border = '2px solid #000'; // Black border for 90s screen aesthetic
    backButton.style.padding = '6px 16px';
    backButton.style.fontFamily = 'Courier New, monospace'; // Monospaced font for a terminal feel
    backButton.style.fontSize = '14px';
    backButton.style.textTransform = 'uppercase'; // All caps for that old-school vibe

    backButton.onclick = function () {
        parentDiv.remove(); // Removes the parentDiv
        toggleDiv.style.display = ''; // Restores the toggleDiv

        // Bring back the soundtrack player
        const playerContainer = document.getElementById("player");
        if (playerContainer) {
            playerContainer.style.display = 'flex'; // Restore the player's visibility
        }
    };

    return backButton;
}
   // Header title with pixelated text (white text)
function createHeaderText(title) {
    const headerTitle = document.createElement('h3');
    headerTitle.innerText = title;
    headerTitle.style.margin = '0';
    headerTitle.style.color = '#FFFFFF'; // White text
    headerTitle.style.fontFamily = 'Courier New, monospace'; // Monospaced font
    headerTitle.style.fontSize = '18px'; // Slightly larger text
    return headerTitle;
}
}

  // Mutation observer setup
  let observerRunning = false;
  let element = document.getElementById('controlElement');
  if (!element) {
      let singleObserver = new MutationObserver(handleMutations);
      singleObserver.observe(document.documentElement, { childList: true, subtree: true });
      let controlElement = document.createElement('div');
      controlElement.style.display = 'none';
      controlElement.id = 'controlElement';
      document.body.appendChild(controlElement);
  }
  
  // Handle DOM mutations to add the button in case the target is not present
  function handleMutations(mutationsList, observer) {
      if (observerRunning) return;
      observerRunning = true;
      addGovernmentButton();
      observer.observe(document.documentElement, { childList: true, subtree: true });
      observerRunning = false;
  }
  
  // Function to refresh relations view
  function refreshRelationsView() {
      let relationsDiv = document.getElementById('relations_view');
      if (relationsDiv) {
          relationsDiv.innerHTML = ''; // Clear the existing content
          setupRelationsArea(relationsDiv, document.querySelector(".inner_window_question"));
      }
  }
  

ev_dict={};
pop_dict={};

  function update_EV_dict() {
    let pop_vote = e.current_results;
    ev_dict={};
    pop_dict={};
    for (let n = 0; n<e.current_results[0].length; n++) {
        ev_dict[e.current_results[0][n].pk] = e.current_results[0][n].evvs;
        pop_dict[e.current_results[0][n].pk] = e.current_results[0][n].pvp;
    }
}
var changeGameDisp = (hide) => {
    disp = "";
    if (hide) {
      disp = "none";
    }
    Array.from(gameWindow.children).forEach(f=>{
        if(!f.classList.contains("game_header") && (f.id != "main_content_area" || !$("#campaign_sign")[0])) 
            f.style.display = disp
    });
  };

e.vpSwitcher = () => {
    let exclude_running_mate_ids = [81, 85, 89, 90]; // Default exclusions

    // Modify exclusions based on conditions
    if (CuomoBeatPataki === 1) {
        console.log("Cuomo has been saved, Cuomo unlocked!");
        exclude_running_mate_ids.push(87); // Example: Add specific ID based on condition
    } else {
        console.log("No Cuomo, Kerry unlocked!");
        exclude_running_mate_ids.push(86); // Exclude different IDs
    }

    // Ensure no duplicates
    exclude_running_mate_ids = [...new Set(exclude_running_mate_ids)];

let potential_running_mate_pks = e.running_mate_json
    .filter(f => !exclude_running_mate_ids.includes(f.fields.running_mate)) // Exclude specific running mates
    .map(f => f.fields.running_mate); // Include all others
    let pot_run_mat = potential_running_mate_pks.map(f=>e.candidate_json.find(_f=>_f.pk===f));
    let running_mate_options = ``;
    pot_run_mat.forEach(f=> {
        running_mate_options += `<option value="${f.pk}">${f.fields.first_name} ${f.fields.last_name}</option>\n`
    });

    let vp_html = `
        <div id="running_mate_form">
            <form name="running mate">
                <p></p>
                <h3>Please select your running mate:</h3> 
                <select name="running_mate_id" id="running_mate_id">
                    ${running_mate_options}
                </select>
                <p></p>
            </form>
        </div>
        <div class="person_description_window" id="running_mate_description_window">
            <div class="person_image" id="running_mate_image"> <img src="${pot_run_mat[0].fields.image_url}" width="210" height="250"> </div>
            <div class="person_summary" id="running_mate_summary">
                <ul>
                    <li>Name: ${pot_run_mat[0].fields.first_name} ${pot_run_mat[0].fields.last_name}</li>
                    <li>Party: ${pot_run_mat[0].fields.party}</li>
                    <li>Home State: ${pot_run_mat[0].fields.state}</li>
                </ul>
                ${pot_run_mat[0].fields.description_as_running_mate}
            </div>
        </div>
        <p> <button class="person_button" id="running_mate_id_button">Continue</button> </p>
    `

    let vp_screen = document.createElement("div");
    vp_screen.classList.add("inner_window_w_desc");
    vp_screen.id = "inner_window_4";
    vp_screen.innerHTML = vp_html;

    changeGameDisp(true);

    gameWindow.appendChild(vp_screen);

    // now we make the stuff do stuff!

    $("#running_mate_id").change((_e)=>{
        _e.preventDefault()

        let value = Number($("#running_mate_id").val());
        let i = potential_running_mate_pks.findIndex(f=>f === value);

        let screen_html = `
            <div class="person_image" id="running_mate_image"> <img src="${pot_run_mat[i].fields.image_url}" width="210" height="250"> </div>
            <div class="person_summary" id="running_mate_summary">
                <ul>
                    <li>Name: ${pot_run_mat[i].fields.first_name} ${pot_run_mat[i].fields.last_name}</li>
                    <li>Party: ${pot_run_mat[i].fields.party}</li>
                    <li>Home State: ${pot_run_mat[i].fields.state}</li>
                </ul>
                ${pot_run_mat[i].fields.description_as_running_mate}
            </div>
        `

        $("#running_mate_description_window").html(screen_html);

    });
    // VP selected!

    $("#running_mate_id_button").click((_e)=>{
        _e.preventDefault();

        let value = Number($("#running_mate_id").val());
        let i = potential_running_mate_pks.findIndex(f=>f === value);

        e.running_mate_last_name = pot_run_mat[i].fields.last_name;
        e.running_mate_image_url = pot_run_mat[i].fields.image_url;
        e.selected_running_mate = pot_run_mat[i];

        if ($("#campaign_sign")[0]) {
            $("#campaign_sign").html(`<p>${e.candidate_last_name}</p><p>${e.running_mate_last_name}</p>`);
            $("#running_mate_pic").attr("src", e.running_mate_image_url)
        }

