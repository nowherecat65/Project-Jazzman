    function addGovernmentButton() {
    if (document.getElementById('government_button')) {
        return;
    }

    const refButton = document.getElementById('view_electoral_map');
    if (!refButton) {
        return;
    }

    const newButton = document.createElement('button');
    newButton.id = 'government_button';
    newButton.innerHTML = 'Government';
    newButton.addEventListener('click', openRelationsView);
    newButton.style.marginLeft = "25px";

    // Apply the red and blue color scheme to the button
    newButton.style.backgroundColor = '#FF0000'; // Red background
    newButton.style.color = '#FFFFFF'; // White text color
    newButton.style.border = '2px solid #0000FF'; // Blue border
    newButton.style.transition = 'background-color 0.3s, border 0.3s'; // Smooth transitions for hover effects

    // Add hover effects to change colors
    newButton.addEventListener('mouseover', function() {
        newButton.style.backgroundColor = '#0000FF'; // Change to blue on hover
        newButton.style.border = '2px solid #FF0000'; // Change border to red
    });

    newButton.addEventListener('mouseout', function() {
        newButton.style.backgroundColor = '#FF0000'; // Change back to red when not hovering
        newButton.style.border = '2px solid #0000FF'; // Change border back to blue
    });

    refButton.insertAdjacentElement('afterend', newButton);
}
    // Function to open the Administration Relations View
    function openRelationsView() {
    newRelationsData = false; // Reset new data indicator
    document.getElementById('government_button').innerHTML = 'Government';
    let questions = document.querySelector(".inner_window_question");
    questions.style.display = 'none';

    let relationsDiv = document.createElement('div');
    relationsDiv.id = 'relations_view';
    relationsDiv.style.height = '100%';
    relationsDiv.style.backgroundColor = '#fff'; // Use white background for retro feel
    relationsDiv.style.padding = '20px';
    relationsDiv.style.fontFamily = "'Courier New', monospace"; // Retro font
    relationsDiv.style.color = '#000'; // Black text for contrast
    setupRelationsArea(relationsDiv, questions);

    questions.parentNode.insertBefore(relationsDiv, questions.nextSibling);
    const playerContainer = document.getElementById("player");
    if (playerContainer) {
        playerContainer.style.display = 'none'; // Hide the player
    }
}

let page_list;  // Declare with let if it's modified later

function setupRelationsArea(parentDiv, toggleDiv) {
    let current_page = 0; // Start on page 0
    page_list = [
        ["Congressional Majority:", "error", "Congressional Approval:", "error", "Public Support:", "error", "Legacy:", "error", "Foreign Reputation:", "error", "Relationship with Hillary:", "error", "Expected Opponent:", "Unknown"]
    ];

    // Function to apply permanent changes once when ans == 1272222
    function applyPermanentChanges() {
        if (ClintonDead == true) {
            page_list[0][6] = "War Situation:";  // Legacy -> War Situation
            page_list[0][10] = "Homeland Security:";  // Relationship with Hillary -> Homeland Security
            console.log("Changes applied: Legacy and RWH deleted and replaced.");
        }
    }

    function styleSpecialWords() {
        // Style special words
        page_list.forEach((page, pageIndex) => {
            page_list[pageIndex] = page.map(content => {
                if (content === "Unknown") {
                    return `<span style="font-weight: bold; color: red; font-size: 24px;">${content}</span>`;
                }
                return content; // Leave other content unchanged
            });
        });
    }
   // Function to update page_list based on conditions
      function updatePageList() {
        // Apply permanent changes before styling
        applyPermanentChanges();
        // Apply styles to the list after applying permanent changes
        styleSpecialWords();
       ans = campaignTrail_temp.player_answers[campaignTrail_temp.player_answers.length - 1];
      if (DemCongress === 1) {
          page_list[0][1] = `<span style="font-weight: bold; color: blue; font-size: 24px;">SLIM DEM MAJORITY</span>`; // Style DEM
          page_list[0][3] = `<span style="font-weight: bold; color: gray; font-size: 24px;"> Average</span>`; // Style Congressional Approval
      } else if (RepublicanRevolution == 1) {
          page_list[0][1] = `<span style="font-weight: bold; color: red; font-size: 24px;"> HUGE GOP MAJORITY</span>`; // Style GOP
          page_list[0][3] = `<span style="font-weight: bold; color: red; font-size: 24px;"> Horrible</span>`; // Style Congressional Approval
      } else if (SlimGOPCongress == 1) {
          page_list[0][1] = `<span style="font-weight: bold; color: red; font-size: 24px;"> SLIM GOP MAJORITY</span>`; // Style GOP
          page_list[0][3] = `<span style="font-weight: bold; color: red; font-size: 24px;"> Bad</span>`; // Style Congressional Approval
      } else if (GOPSupermajority == 1) {
          page_list[0][1] = `<span style="font-weight: bold; color: red; font-size: 24px;"> GOP SUPERMAJORITY</span>`; // Style GOP
          page_list[0][3] = `<span style="font-weight: bold; color: red; font-size: 24px;"> Abysmal</span>`; // Style Congressional Approval
      } if (Legacy === 0) {
          page_list[0][7] = `<span style="font-weight: bold; color: black; font-size: 24px;"> None</span>`; // Style Legacy
      } else if (Legacy >= 1 && Legacy <= 2) {
          page_list[0][7] = `<span style="font-weight: bold; color: red; font-size: 24px;"> Low</span>`; // Style Legacy
        } else if (Legacy == 3) {
          page_list[0][7] = `<span style="font-weight: bold; color: teal; font-size: 24px;"> Decent</span>`; // Style Legacy
      } else if (Legacy >= 4 && Legacy <= 5) {
          page_list[0][7] = `<span style="font-weight: bold; color: blue; font-size: 24px;"> Great</span>`; // Style Legacy
      } else if (Legacy === 6) {
          page_list[0][7] = `<span style="font-weight: bold; color: gold; font-size: 24px;"> Perfect</span>`; // Style Legacy
      } if (PublicSupport <= 0) {
          page_list[0][5] = `<span style="font-weight: bold; color: red; font-size: 24px;"> Abysmal</span>`; // Style PublicSupport
        } else if (PublicSupport >= 1 && PublicSupport <= 5) {
          page_list[0][5] = `<span style="font-weight: bold; color: red; font-size: 24px;"> Low</span>`; // Style PublicSupport
        } else if (PublicSupport >= 6 && PublicSupport <= 10) {
          page_list[0][5] = `<span style="font-weight: bold; color: gray; font-size: 24px;"> Medium</span>`; // Style PublicSupport
        } else if (PublicSupport >= 11 && PublicSupport <= 15) {
          page_list[0][5] = `<span style="font-weight: bold; color: gray; font-size: 24px;"> High</span>`; // Style PublicSupport
        } else if (PublicSupport >= 16 && PublicSupport <= 20) {
          page_list[0][5] = `<span style="font-weight: bold; color: gray; font-size: 24px;"> Very High</span>`; // Style PublicSupport
      } if (FPWin <= 0) {
          page_list[0][9] = `<span style="font-weight: bold; color: red; font-size: 24px;"> Laughingstock</span>`; // Style ForeignReputation
      } else if (FPWin >= 1 && FPWin <=3) {
          page_list[0][9] = `<span style="font-weight: bold; color: green; font-size: 24px;"> Respected</span>`; // Style ForeignReputation
      } else if (FPWin >= 4 && FPWin <=5) {
